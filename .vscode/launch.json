    "version": "0.2.0",
    "configurations": [
        {
            "name": "Bot",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/bot/main.go",
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "name": "Build Configurator",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/configurator/",
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "name": "File",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}",
            "envFile": "${workspaceFolder}/.env"
        }
    ]
}