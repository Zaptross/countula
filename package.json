{
    "name": "counting-bot",
    "version": "0.0.1",
    "description": "A bot to run a discord counting game. Slightly evil.",
    "main": "index.js",
    "watch": {
        "rebuild": {
            "patterns": [
                "src"
            ],
            "extensions": "ts,json",
            "ignore": [
                "dist",
                "node_modules"
            ]
        }
    },
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "tsc -b",
        "watch": "npm-watch rebuild",
        "rebuild": "tsc -b && @powershell cp './src/*.json' ./dist/",
        "start": "node ./dist/index.js"
    },
    "keywords": [
        "discord",
        "bot",
        "counting",
        "game"
    ],
    "author": "Matthew Price",
    "license": "MIT",
    "devDependencies": {
        "@types/number-to-words": "^1.2.1",
        "@types/romans": "^1.0.0",
        "@types/luxon": "^2.0.0",
        "@types/node": "^16.4.13",
        "@typescript-eslint/eslint-plugin": "^4.29.2",
        "@typescript-eslint/parser": "^4.29.2",
        "eslint": "^7.32.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-plugin-import": "^2.24.0",
        "prettier": "^2.3.2",
        "ts-node": "^10.1.0",
        "typescript": "^4.3.5"
    },
    "dependencies": {
        "discord.js": "^12.5.3",
        "dotenv": "^10.0.0",
        "luxon": "^2.0.2",
        "npm-watch": "^0.11.0",
        "number-to-words": "^1.2.4",
        "romans": "^1.1.2",
        "ts-node-dev": "^1.1.8"
    }
}
