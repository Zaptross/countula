{
  "name": "counting-bot",
  "version": "0.0.1",
  "description": "A bot to run a discord counting game. Slightly evil.",
  "main": "index.js",
  "watch": {
    "index": {
      "patterns": [
        "./src"
      ],
      "extensions": "js,ts"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc -b",
    "watch": "npm-watch reload",
    "reload": "npm run build && npm run start",
    "start": "node ./dist/index.js"
  },
  "keywords": [
    "discord",
    "bot",
    "counting",
    "game"
  ],
  "author": "Matthew Price",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.4.13",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@types/number-to-words": "^1.2.1",
    "discord.js": "^12.5.3",
    "dotenv": "^10.0.0",
    "npm-watch": "^0.11.0",
    "number-to-words": "^1.2.4",
    "ts-node-dev": "^1.1.8"
  }
}